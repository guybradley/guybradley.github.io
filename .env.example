# MailerSend Configuration
# Copy this file to .env and fill in your values

# MailerSend API key (get this from your MailerSend dashboard)
MAILERSEND_API_KEY=

# Email sender details
EMAIL_FROM_ADDRESS=website@soulcountry.app
EMAIL_FROM_NAME=SoulCountry Website

# Email recipient details (where form submissions will be sent)
EMAIL_TO=info@soulcountry.app
EMAIL_TO_NAME=SoulCountry Digital

# Node environment
NODE_ENV=production